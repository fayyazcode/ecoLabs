{
  "compilerOptions": {
    "target": "ES2022",
    "rootDir": "./src",
    "moduleResolution": "Node16",
    "module": "Node16",
    // "baseUrl": "./src",
    // "paths": {
    //   "@/*": ["*"]
    // },
    "outDir": "./dist",
    "noEmit": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "moduleSuffixes": [".js", ""]
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
